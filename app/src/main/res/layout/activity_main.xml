<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    android:background="@color/background"
    android:orientation="vertical"
    tools:context=".MainActivity"
    tools:ignore="ExtraText">

    <TextView
        android:id="@+id/Form"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/trebuc"
        android:text="Personal Form"
        android:textColor="@color/textColor"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/onetext"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:fontFamily="@font/trebuc"
        android:hint="Name"
        android:importantForAutofill="no"
        android:inputType="text"
        android:textColor="@color/textColor"
        android:textSize="14sp"
        tools:ignore="HardcodedText,LabelFor,VisualLintTextFieldSize" />

    <EditText
        android:id="@+id/secondtext"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:fontFamily="@font/trebuc"
        android:hint="Mobile Number"
        android:importantForAutofill="no"
        android:inputType="numberDecimal"
        android:textColor="@color/textColor"
        android:textSize="14sp"
        tools:ignore="HardcodedText,LabelFor,VisualLintTextFieldSize" />

    <TextView
        android:id="@+id/Course"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:fontFamily="@font/trebuc"
        android:text="Course*"
        android:textColor="@color/textColor"
        android:textSize="14sp"
        tools:ignore="HardcodedText" />.

    <CheckBox
        android:id="@+id/checkboxone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/trebuc"
        android:minWidth="48dp"
        android:minHeight="48dp"
        android:text="C"
        android:textColor="@color/textColor"
        android:textSize="14sp"
        tools:ignore="HardcodedText" />.

    <CheckBox
        android:id="@+id/checkboxtwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/trebuc"
        android:minHeight="48dp"
        android:text="C++"
        android:textColor="@color/textColor"
        android:textSize="14sp"
        tools:ignore="HardcodedText" />.

    <CheckBox
        android:id="@+id/chkboxthree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/trebuc"
        android:minHeight="48dp"
        android:text="Python"
        android:textColor="@color/textColor"
        android:textSize="14sp"
        tools:ignore="HardcodedText" />.

    <CheckBox
        android:id="@+id/chkboxfour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/trebuc"
        android:minHeight="48dp"
        android:text="Java"
        android:textColor="@color/textColor"
        android:textSize="14sp"
        tools:ignore="HardcodedText" />.

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <CheckBox
            android:id="@+id/chkboxfive"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/trebuc"
            android:minHeight="48dp"
            android:text="Other"
            android:textColor="@color/textColor"
            android:textSize="14sp"
            tools:ignore="HardcodedText" />

        <EditText
            android:id="@+id/threetext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:fontFamily="@font/trebuc"
            android:hint="@string/mention_here"
            android:importantForAutofill="no"
            android:inputType="text"
            android:textColor="@color/textColor"

            android:textSize="14sp"
            tools:ignore="LabelFor,SpeakableTextPresentCheck,VisualLintTextFieldSize" />.
    </LinearLayout>

    <EditText
        android:id="@+id/fourtext"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/trebuc"
        android:hint="Email*"
        android:importantForAutofill="no"
        android:textColor="@color/textColor"
        android:textSize="14sp"
        tools:ignore="HardcodedText,LabelFor,TextFields,VisualLintTextFieldSize" />.

    <EditText
        android:id="@+id/fivetext"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:fontFamily="@font/trebuc"
        android:hint="Address"
        android:importantForAutofill="no"
        android:textColor="@color/textColor"
        android:textSize="14sp"
        tools:ignore="HardcodedText,LabelFor,VisualLintTextFieldSize" />.

    <Button
        android:id="@+id/onebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp"
        android:onClick="submit"
        android:text="Submit"
        android:textSize="14sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText,UsingOnClickInXml" />.


</LinearLayout>



